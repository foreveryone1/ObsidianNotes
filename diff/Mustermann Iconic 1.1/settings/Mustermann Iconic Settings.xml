	</set_global>


	<!-- This allows us to set a global font adjustment value for given languages this 
	     is required because we map to a different font set for russian and greek which
		 ends up with a larger vertical size then the base font which causes cut offs, so now we
		 can adjust it to fit closer to the original size -->
	<list id="language_font_scaling_list">
		<record name="russian">
			<coordinate id="value" x="0.9" y="0.9" />
		</record>
		<record name="greek">
			<coordinate id="value" x="0.9" y="0.9" />
		</record>
		<record name="default">
			<coordinate id="value" x="1.0" y="1.0" />
		</record>
	</list>

